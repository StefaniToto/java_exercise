CREATE TABLE classwork."T_Product"
(

  n_idproduct integer NOT NULL,
  t_nome character varying(100),
  t_brand character varying(100),
  n_price numeric,
  n_quantity integer,
  PRIMARY KEY (n_idproduct)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE classwork."T_Product"
  OWNER TO orfin;


CREATE TABLE classwork."T_Order"
(

  n_idorder integer NOT NULL,
  d_order_date DATE,
  n_idcustomer integer,
  t_ship_address character varying(100),
 n_total_amount numeric,
  PRIMARY KEY (n_idorder)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE classwork."T_Order"
  OWNER TO orfin;

CREATE TABLE classwork."T_Order_Detail"
(

  n_iddetail integer NOT NULL,
n_idorder integer,
n_idproduct integer,
n_quantity integer,
 n_unitprice numeric,
  PRIMARY KEY (n_iddetail)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE classwork."T_Order_Detail"
  OWNER TO orfin;

  CREATE SEQUENCE product_seq
start with 1
increment by 1
minvalue 0
maxvalue 100
cycle;

 CREATE SEQUENCE customer_seq
start with 1
increment by 1
minvalue 0
maxvalue 100
cycle;

 CREATE SEQUENCE order_seq
start with 1
increment by 1
minvalue 0
maxvalue 100
cycle;

 CREATE SEQUENCE orderdetail_seq
start with 1
increment by 1
minvalue 0
maxvalue 100
cycle;


